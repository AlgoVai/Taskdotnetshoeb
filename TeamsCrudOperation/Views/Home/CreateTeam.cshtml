@model TeamsCrudOperation.Models.TeamInfo;
@{
    ViewData["Title"] = "CreateTeam";
}

<h1>CreateTeam</h1>

<form asp-controller="Home" asp-action="CreateTeam" method="post">
    <div class="form-group">
        <label asp-for="TeamName"></label>
        <input asp-for="@Model.TeamName" class="form-control" />
        <span asp-validation-for="TeamName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Description"></label>
        <input asp-for="@Model.Description" class="form-control" />
        <span asp-validation-for="Description" class="text-danger"></span>
    </div>
    </br>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

